/**
 * @function calculerSomme
 * @param {number} a Le premier nombre à additionner.
 * @param {number} b Le deuxième nombre à additionner.
 * @returns {number} La somme des deux nombres. c'est le return
 */
function calculerSomme(a, b) {
    return a + b;
}

/**
 * @function calculerDifférence
 * @param {number} c Le premier nombre à soustrainre.
 * @param {number} d Le deuxième nombre à soustraire.
 * @returns {number} La différence des deux nombres.
 */
function calculerDiff(a, b) {
    return a - b;
}